{
  "name": "cypress-vscode-snippets",
  "publisher": "admah",
  "displayName": "Cypress VS Code Snippets",
  "description": "Cypress snippets extension for VS Code",
  "version": "0.1",
  "engines": {
    "vscode": "^1.56.0"
  },
  "scripts": {
    "package": "npx vsce package",
    "publish": "npx vsce publish",
    "cm": "cz"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/admah/cypress-vscode-snippets"
  },
  "bugs": {
    "url": "https://github.com/admah/cypress-vscode-snippets/issues"
  },
  "homepage": "https://github.com/admah/cypress-vscode-snippets#readme",
  "icon": "icons/logo.png",
  "galleryBanner": {
    "color": "#0b6f47",
    "theme": "dark"
  },
  "keywords": [
    "cypress",
    "e2e",
    "testing",
    "vscode"
  ],
  "categories": [
    "Snippets"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/snippets.json"
      },
      {
        "language": "javascriptreact",
        "path": "./snippets/snippets.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/snippets.json"
      }
    ]
  },
  "devDependencies": {
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^8.0.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && npx cz --hook || true"
    }
  }
}
